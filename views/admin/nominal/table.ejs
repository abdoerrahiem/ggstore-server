<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No.</th>
      <th>Coin name</th>
      <th>Total coin</th>
      <th>Price</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < nominals.length; i++){ %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= nominals[i].coinName %></td>
      <td><%= nominals[i].coinQuantity %></td>
      <td><%= nominals[i].price %></td>
      <td>
        <form
          action="/nominal/delete/<%= nominals[i]._id %>?_method=DELETE"
          method="POST"
        >
          <a
            href="/nominal/edit/<%= nominals[i]._id %>"
            class="btn btn-info btn-sm"
          >
            <i class="fas fa-pencil-alt"></i>
            Update
          </a>
          <button class="btn btn-danger btn-sm">
            <i class="fas fa-trash"></i>
            Delete
          </button>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
